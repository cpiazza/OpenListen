<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:background="@drawable/white"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent">
  
  <TextView
    	android:padding="10dp"
    	android:text="My Rankings"
    	android:textColor="@drawable/black"
    	android:textStyle="bold"
    	android:textSize="18sp"
      	android:layout_width="fill_parent"
    	android:layout_height="wrap_content"
    />
  <ScrollView
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:orientation="vertical"
	android:scrollbars="vertical">
    
    <TableLayout
    	android:layout_width="fill_parent"
    	android:layout_height="wrap_content">
    	<TableRow
    		android:id="@+id/myRankRow1"
    		android:clickable="true"    		
    		android:orientation="vertical"
    		android:paddingBottom="15dp"
    		android:layout_width="fill_parent"
    		android:layout_height="wrap_content">
    		
    		<LinearLayout
  				android:orientation="horizontal"
  				android:layout_width="wrap_content"
  				android:layout_height="wrap_content">
    		
    		<ImageView
    		    android:id="@+id/imgPres"
    			android:paddingRight="15dp"
    			android:paddingLeft="10dp"
    			android:src="@drawable/pres_50"
    			android:layout_width="wrap_content"
    			android:layout_height="wrap_content"
    		/>
    		<LinearLayout
  				android:orientation="vertical"
  				android:layout_width="wrap_content"
  				android:layout_height="wrap_content">
  				<TextView
					android:id="@+id/txtPresidentHeader"
					android:text="President"
    				android:textStyle="bold"
    				android:textSize="16sp"					
    				android:layout_width="fill_parent"
    				android:layout_height="wrap_content"
	    			android:textColor="@drawable/black"
	    		/>  				
    			<TextView
					android:id="@+id/txtPresident"
					android:paddingRight="8sp"
    				android:layout_width="wrap_content"
    				android:layout_height="wrap_content"
	    			android:textColor="@drawable/black"    		
    			/>
    		</LinearLayout>
    		</LinearLayout>
    	</TableRow>
    	<TableRow
    		android:orientation="vertical"
    		android:id="@+id/myRankRow2"
    		android:clickable="true"      		
    		android:paddingBottom="15dp"
    		android:layout_width="wrap_content"
    		android:layout_height="wrap_content">
    		
    		<LinearLayout
  				android:orientation="horizontal"
  				android:layout_width="wrap_content"
  				android:layout_height="wrap_content">    		
    		
    		<ImageView
    		    android:id="@+id/imgVP"
    			android:paddingRight="15dp"
    			android:paddingLeft="10dp"
    			android:src="@drawable/vp_50"
    			android:layout_width="wrap_content"
    			android:layout_height="wrap_content"
    		/>
    		<LinearLayout
  				android:orientation="vertical"
  				android:layout_width="wrap_content"
  				android:layout_height="wrap_content">
  				<TextView
					android:id="@+id/txtVicePresidentHeader"
					android:text="Vice President"
    				android:textStyle="bold"
    				android:textSize="16sp"					
    				android:layout_width="wrap_content"
    				android:layout_height="wrap_content"
	    			android:textColor="@drawable/black"
	    		/>  				
    			<TextView
					android:id="@+id/txtVicePresident"
					android:paddingRight="8sp"
    				android:layout_width="wrap_content"
    				android:layout_height="wrap_content"
	    			android:textColor="@drawable/black"    		
    			/>
    		</LinearLayout>
    		</LinearLayout>
    	</TableRow>
    	<TableRow
    		android:orientation="vertical"
			android:id="@+id/myRankRow3"    		
			android:clickable="true"    		
    		android:paddingBottom="15dp"
    		android:layout_width="wrap_content"
    		android:layout_height="wrap_content">
    		
    		<LinearLayout
  				android:orientation="horizontal"
  				android:layout_width="wrap_content"
  				android:layout_height="wrap_content">    		
    		
    		<ImageView
    		    android:id="@+id/imgLieutenant"
    			android:paddingRight="15dp"
    			android:paddingLeft="10dp"
    			android:src="@drawable/lieutenant_50"
    			android:layout_width="wrap_content"
    			android:layout_height="wrap_content"
    		/>
    		<LinearLayout
  				android:orientation="vertical"
  				android:layout_width="wrap_content"
  				android:layout_height="wrap_content">
  				<TextView
					android:id="@+id/txtLieutenantHeader"
					android:text="Lieutenant"
    				android:textStyle="bold"
    				android:textSize="16sp"					
    				android:layout_width="wrap_content"
    				android:layout_height="wrap_content"
	    			android:textColor="@drawable/black"
	    		/>  				
    			<TextView
					android:id="@+id/txtLieutenant"
					android:paddingRight="8sp"
    				android:layout_width="wrap_content"
    				android:layout_height="wrap_content"
	    			android:textColor="@drawable/black"    		
    			/>
    		</LinearLayout>
    		</LinearLayout>
    	</TableRow>
    	<TableRow
    		android:orientation="vertical"
			android:id="@+id/myRankRow4"    		
			android:clickable="true"       		
    		android:paddingBottom="15dp"
    		android:layout_width="wrap_content"
    		android:layout_height="wrap_content">
    		<LinearLayout
  				android:orientation="horizontal"
  				android:layout_width="wrap_content"
  				android:layout_height="wrap_content">    		
    		
    		<ImageView
    		    android:id="@+id/imgPrivate"
    			android:paddingRight="15dp"
    			android:paddingLeft="10dp"
    			android:src="@drawable/private_50"
    			android:layout_width="wrap_content"
    			android:layout_height="wrap_content"
    		/>
    		<LinearLayout
  				android:orientation="vertical"
  				android:layout_width="wrap_content"
  				android:layout_height="wrap_content">
  				<TextView
					android:id="@+id/txtPrivateHeader"
					android:text="Private"
    				android:textStyle="bold"
    				android:textSize="16sp"					
    				android:layout_width="wrap_content"
    				android:layout_height="wrap_content"
	    			android:textColor="@drawable/black"
	    		/>  				
    			<TextView
					android:id="@+id/txtPrivate"
					android:paddingRight="8sp"
    				android:layout_width="wrap_content"
    				android:layout_height="wrap_content"
	    			android:textColor="@drawable/black"    		
    			/>
    		</LinearLayout>
    		</LinearLayout>
    	</TableRow>
    	<TableRow
    		android:orientation="vertical"
			android:id="@+id/myRankRow5"    		
			android:clickable="true"       		
    		android:paddingBottom="15dp"
    		android:layout_width="wrap_content"
    		android:layout_height="wrap_content">
    		<LinearLayout
  				android:orientation="horizontal"
  				android:layout_width="wrap_content"
  				android:layout_height="wrap_content">    		
    		
    		<ImageView
    		    android:id="@+id/imgCivilian"
    			android:paddingRight="15dp"
    			android:paddingLeft="10dp"
    			android:src="@drawable/civ_50"
    			android:layout_width="wrap_content"
    			android:layout_height="wrap_content"
    		/>
    		<LinearLayout
  				android:orientation="vertical"
  				android:layout_width="wrap_content"
  				android:layout_height="wrap_content">
  				<TextView
					android:id="@+id/txtCivilianHeader"
					android:text="Civilian"
    				android:textStyle="bold"
    				android:textSize="16sp"					
    				android:layout_width="wrap_content"
    				android:layout_height="wrap_content"
	    			android:textColor="@drawable/black"
	    		/>  				
    			<TextView
					android:id="@+id/txtCivilian"
					android:paddingRight="8sp"
    				android:layout_width="wrap_content"
    				android:layout_height="wrap_content"
	    			android:textColor="@drawable/black"    		
    			/>
    		</LinearLayout>
    		</LinearLayout>
    	</TableRow>    	
    </TableLayout>
	</ScrollView>    
  
</LinearLayout>
